{
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_ENV": "production"
    }
  },
  "outputDirectory": "out",
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "target": "static",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 